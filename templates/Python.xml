<templateSet group="Python">
  <template name="pp" value="print('Debug --&gt;', $VAR$)" description="Print beach !!!" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="ppp" value="import json&#10;print(json.dumps($VAR$, indent=3))" description="Print beach !!!" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="_set" value="[x for x in dir($VAR$) if x.endswith('_set')]" description="print set from obj" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="ppc" value="print('{:#^80}'.format($VAR$))" description="Print center beach !!!" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="spliter" value="# _____________________________________________________________________" description="split code comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>